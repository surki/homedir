alt + shift + e
    xdotool search --name "emacs@${HOSTNAME}" windowactivate

alt + shift + c
    xdotool search --name "${USER}@${HOSTNAME}.*byobu" windowactivate

alt + shift + w
    xdotool search --name ".* Mozilla Firefox" windowactivate

alt + shift + g
    xdotool search --name ' - Chromium' windowactivate
